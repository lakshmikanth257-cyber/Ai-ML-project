# Transport configuration
ASYA_TRANSPORT=sqs  # sqs or rabbitmq

# Runtime configuration
# Note: Socket path is hard-coded to /var/run/asya/asya-runtime.sock (managed by operator)
ASYA_RUNTIME_TIMEOUT=5m

# End queues
ASYA_ACTOR_HAPPY_END=happy-end
ASYA_ACTOR_ERROR_END=error-end

# SQS Configuration (when ASYA_TRANSPORT=sqs)
ASYA_QUEUE_BASE_URL=https://sqs.us-east-1.amazonaws.com/123456789012
ASYA_AWS_REGION=us-east-1
ASYA_SQS_VISIBILITY_TIMEOUT=300
ASYA_SQS_WAIT_TIME_SECONDS=20
ASYA_SQS_MAX_MESSAGES=1

# RabbitMQ Configuration (when ASYA_TRANSPORT=rabbitmq)
ASYA_RABBITMQ_URL=amqp://guest:guest@localhost:5672/
ASYA_RABBITMQ_EXCHANGE=asya
ASYA_RABBITMQ_PREFETCH=1
